---
config:
  flowchart:
    curve: linear
---
graph TD;
	__start__([<p>__start__</p>]):::first
	initialize(initialize)
	planning(planning)
	tools(tools)
	reflection(reflection)
	finalize(finalize)
	__end__([<p>__end__</p>]):::last
	__start__ --> initialize;
	initialize --> planning;
	planning -.-> finalize;
	planning -.-> tools;
	reflection -.-> finalize;
	reflection -.-> planning;
	reflection -.-> tools;
	tools --> reflection;
	finalize --> __end__;
	classDef default fill:#f2f0ff,line-height:1.2
	classDef first fill-opacity:0
	classDef last fill:#bfb6fc
